{
  // "extends": "@xjjrtz/tsconfig/tsconfig.base.json",
  "display": "Base",
  "compilerOptions": {
    "composite": false,
    "jsx": "preserve",
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "strictFunctionTypes": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "declaration": true,
    "declarationMap": true,
    "inlineSources": false,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "target": "esnext",
    "baseUrl": ".",
    "module": "esnext"
  },
  "exclude": ["**/node_modules/**", "**/dist/**"],
  // "compilerOptions": {
  //   "target": "esnext",
  //   "baseUrl": ".",
  //   "module": "esnext"
  // },
  "include": ["src"]
}
